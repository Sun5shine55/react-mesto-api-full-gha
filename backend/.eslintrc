{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "linebreak-style": 0,
    "eslint-linebreak-style": [
      0,
      "error",
      "windows"
    ],
    "global-require": 0,
    "no-console": "warn"
  }
}